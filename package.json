{
  "name": "cazorla-magica",
  "version": "1.0.0",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "parcel:dev": "parcel --target web",
    "parcel:build": "parcel build --target web",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache"
  },
  "keywords": [],
  "author": "Andrés Terol Sánchez <andrests@uoc.edu>",
  "license": "ISC",
  "description": "Cazorla web page for HTML & CSS I development tools. PEC 1.",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "source": "src/index.html"
    }
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.13.0",
    "postcss": "^8.4.49",
    "postcss-custom-properties": "^14.0.4",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.0.1"
  }
}
